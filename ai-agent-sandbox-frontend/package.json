{
  "name": "ai-agent-sandbox-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 7777 --turbopack",
    "build": "next build --turbopack",
    "start": "next start -p 7777",
    "lint:prettier": "prettier --check --log-level=warn '**/!(*.min|*-lock).{yaml,yml,md,mdx}'",
    "prettier": "prettier --write --log-level=warn '**/!(*.min|*-lock).{yaml,yml,md,mdx}'",
    "lint": "ultracite check && npm run lint:prettier",
    "format": "ultracite fix && npm run prettier",
    "sync:agents-docs": "npx tsx src/scripts/copy-agents-markdown.ts"
  },
  "dependencies": {
    "@heroui/react": "2.8.5",
    "framer-motion": "12.23.24",
    "next": "15.5.4",
    "next-themes": "0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "recharts": "3.3.0",
    "usehooks-ts": "3.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@iconify/react": "6.0.2",
    "@tailwindcss/postcss": "4.1.14",
    "@types/node": "24.7.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.14",
    "typescript": "5.9.3",
    "ultracite": "5.6.2"
  }
}
